// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//
model Realty {
  id                String  @id @default(cuid())
  area              Float?
  buildingType      String?
  buildingName      String?
  category          String?
  creationDate      String?
  lastUpdateDate    String?
  description       String?
  floor             Int?
  floorsTotal       Int?
  windowView        String?
  parkingType       String?
  balcony           String?
  image             Json?
  location          Json?
  lotArea           Float?
  manuallyAdded     Int?
  mortgage          Int?
  payedAdv          Int?
  utilitiesIncluded Int?
  phone             Int?
  internet          Int?
  rubbishChute      Int?
  lift              Int?
  price             Float?
  propertyType      String?
  renovation        String?
  type              String?
  dealStatus        String?
  waterSupply       Int?
  electricitySupply Int?
  gasSupply         Int?
  heatingSupply     Int?
  lotType           String?
  kitchenSpace      Float?
  livingSpace       Float?
  newFlat           Int?
  garageType        String?
  purpose           String?
  rooms             Int?
  roomsType         String?
  ceilingHeight     Float?
  bathroomUnit      String?
  videoReview       String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([category])
}

